
public class Cleric {
	String name;
	int hp=50;
	final int MAXHP=50;
	int mp=10;
	final int MAXMP=10;

		public void selfAid(){
			this.mp -=5;
			this.hp=this.MAXHP;
			System.out.println(this.name+"は、セルフエイドを唱えた！");
			System.out.println(this.name+"は、MPを5消費した！");
			System.out.println(this.name+"のHPが全回復した！");



	}

}
