
public class HelloWorld {

	public static void main(String[] args) {

		int seibetsu =1;
		int age = 25;
		System.out.println("こんにちは");
		if(seibetsu==0){
			System.out.println("私は男です");
		}else{
			System.out.println("私は女です");
		}
		if(seibetsu==0){
			System.out.println(age + "歳です。");
		}
		System.out.println("よろしくお願いします。");
	}
}